<?xml version="1.0" encoding="UTF-8"?>

<io-channels
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="io_channel.xsd">

	<input-channels>
		<tcp-client-channel id="input_1">
			<local-address host="localhost" port="0" />
			<remote-address host="localhost" port="5000" />
		</tcp-client-channel>

		<tcp-client-channel id="input_2">
			<local-address host="localhost" port="0" />
			<remote-address host="localhost" port="5000" />
		</tcp-client-channel>
	</input-channels>

	<output-channels>
		<tcp-server-channel id="output">
			<local-address host="localhost" port="5000" />
		</tcp-server-channel>
	</output-channels>

</io-channels>     